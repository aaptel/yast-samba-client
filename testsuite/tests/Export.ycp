// test for Samba::Export - autoyast
// $Id$
// testedfiles:Samba.ycp

    include "testsuite.ycp";
    
    import "Samba";

    TESTSUITE_INIT([
	$[ "init" : $[ "scripts" : $[ "exists" : true, "runlevel" : $[] ] ] ]
    ], nil);

    // Export is in autoyast only
    import "Mode";
    Mode::config = true;
    
    Samba::setWorkgroup( "testing" );
    Samba::setWinbind( true );
    
    TEST( ``(Samba::Export() ), [], nil );

    Samba::setWorkgroup( "othertesting" );
    Samba::setWinbind( false );
    
    TEST( ``(Samba::Export() ), [], nil );
}
