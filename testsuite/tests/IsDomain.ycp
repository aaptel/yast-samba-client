{

// test for Samba::isDomain(domain)
// $Id$

    include "testsuite.ycp";
    
    import "Samba";

    Samba::nmbstatus_running = false;
    Samba::nmbstatus_output = $[
        "DEBIAN_FANS" : $[
            "LMB": "BRAHMA",
            "MEMBERS": "BRAHMA"
        ],
        "FAXSERVER" : $[
            "LMB":      "TEST1",
            "MEMBERS":  "TEST1"
        ],
        "MYDOMAIN" : $[
            "PDC" : "SUSIK",
            "DMB" : "SUSIK",
            "LMB" : "SUSIK",
            "MEMBERS" : "SUSIK"
        ]
    ];

    // reasonable result, is not domain
    TEST( ``( Samba::isDomain("FAXSERVER") ), [], nil );

    // reasonable result, is domain
    TEST( ``( Samba::isDomain("MYDOMAIN") ), [], nil );

}
